<ElementCard@MDCard>:
    md_bg_color: 120/255, 173/255, 173/255, 1
    padding:dp(10)
    spacing:dp(10)
    radius:dp(25)
    ripple_behavior: True
    image:''
    text:""
    items_count:""
    subtext:''

    orientation:'vertical'
    MDBoxLayout:
        orientation:'vertical'
        MDLabel:
            halign:"center"
            text:root.text
            font_size: "15sp" 
