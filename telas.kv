            text_color: 0, 0, 0
            font_size:"20sp"
            font_name: "Poppins-SemiBold.ttf"

        MDGridLayout:
            size_hint_y:.55
            cols:2
            padding:[dp(15),dp(15),dp(15),dp(35)]
            spacing:dp(13)
            ElementCard:
                text:"Email"
                on_press: app.root.current = 'curso_text' 
                
            ElementCard:
                text:"Sites Falsos"
                on_press: app.root.current = 'curso_text' 
                
            ElementCard:
                text:"Redes Sociais"
                on_press: app.root.current = 'curso_text' 

            ElementCard:
                text:"Telefone e SMS"
                on_press: app.root.current = 'curso_text'  
                
<ElementCard@MDCard>:
    md_bg_color: 120/255, 173/255, 173/255, 1
    padding:dp(10)
    spacing:dp(10)
    radius:dp(25)
    ripple_behavior: True
    image:''
    text:""
    items_count:""
    subtext:''

    orientation:'vertical'
    MDBoxLayout:
        orientation:'vertical'
        MDLabel:
            halign:"center"
            text:root.text
            font_size: "15sp" 
            font_name: "Poppins-Medium.ttf"
        MDLabel:
            halign:"center"
            font_style:"Caption"
            text:root.subtext
        MDLabel:
            halign:"center"
            text:root.items_count

<CourseScreen>:
    name: 'curso_text'
    MDFloatLayout:
        md_bg_color: 245, 245, 245,1
        Image:
            source: "images/logo.png"
            size_hint: .50, .50
            pos_hint: {"center_x": .52, "center_y": .8}
        
        MDIconButton:
            icon: "chevron-right"
            user_font_size: "50sp"
            pos_hint: {"center_x": .8, "center_y": .12}      
            on_press: app.root.current = 'curso_questao'   

        MDLabel:
            text: "A Engenharia Social por Telefone é uma estratégia de manipulação psicológica em que os golpistas usam chamadas telefônicas para enganar as vítimas. Eles se apresentam como pessoas de confiança, como representantes de empresas, autoridades ou técnicos de suporte, com o objetivo de obter informações pessoais sensíveis ou convencer as vítimas a realizar ações prejudiciais. Táticas como persuasão, intimidação e simpatia são frequentemente usadas. É de extrema importância verificar a autenticidade das chamadas, nunca compartilhar informações confidenciais e não seguir instruções suspeitas por telefone para evitar cair em armadilhas desse tipo."   
            pos_hint: {"center_x": .5, "center_y": .4}       
            halign: "center"
            theme_text_color: "Custom"
            text_color: 0, 0, 0
            font_size:"13sp"
            font_name: "Poppins-SemiBold.ttf"
         
